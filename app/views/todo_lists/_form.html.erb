<%= form_for(@todo_list) do |f| %>
  <% if @todo_list.errors.any? %>
    <div class="flash-alert">
      <% @todo_list.errors.full_messages.each do |message| %>
      <%= message %>
      <% end %>
    </div>
  <% end %>
<br>
  <div class="field">
    <%= f.label :list_name %><br>
    <%= f.text_field :list_name %>
  </div>
  <div class="field">
    <%= f.label :list_due_date %><br>
    <%= f.date_select :list_due_date %>
  </div>
  <br>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
